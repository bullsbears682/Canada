{
  "name": "data-service-orchestrator",
  "version": "1.0.0",
  "description": "Enterprise Data Service Orchestrator with 120+ methods",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run build:frontend && npm run build:functions",
    "build:netlify": "npm run build:frontend && npm run build:functions",
    "build:frontend": "mkdir -p public && cp -r src/frontend/* public/",
    "build:functions": "mkdir -p netlify/functions && cp -r src/functions/* netlify/functions/",
    "test": "jest",
    "lint": "eslint src/**/*.js"
  },
  "keywords": [
    "enterprise",
    "data-service",
    "orchestrator",
    "monitoring",
    "analytics",
    "performance"
  ],
  "author": "Enterprise Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.5.0",
    "eslint": "^8.40.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}